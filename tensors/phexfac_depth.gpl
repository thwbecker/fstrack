set ylabel "{/Symbol d}"
set xlabel "depth [km]"
plot 'hexfac_dir/hex.1.0.7.0.dat' using 1 : 14 title "old, Voigt" w l,\
	'hexfac_dir/hex.1.0.7.0.5.dat' using 1 : 14 title "old, VHR" w l,\
	'hexfac_dir/hex.1.0.7.1.dat' using 1 : 14 title "old, Reuss" w l,\
	'hexfac_dir/hex.14.0.7.0.dat' using 1 : 14 title "new, Voigt" w l,\
	'hexfac_dir/hex.14.0.7.0.5.dat' using 1 : 14 title "new, VHR" w l,\
